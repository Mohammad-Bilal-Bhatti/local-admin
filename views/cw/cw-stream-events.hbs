

<h3>Stream: {{groupName}} - {{streamName}}</h3>

<h4 class="mt-2">Events:</h4>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Timestamp</th>
            <th scope="col">Message</th>
            <th scope="col">Ingestion Time</th>
        </tr>
    </thead>
    <tbody>
        {{#each events}}
        <tr>
            <td>{{timestamp}}</td>
            <td>{{message}}</td>
            <td>{{ingestionTime}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>

