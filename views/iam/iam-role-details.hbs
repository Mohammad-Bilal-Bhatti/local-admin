
<h2>Role Details - {{roleName}}</h2>

<a href="/iam/delete-role?roleName={{roleName}}" role="button" class="btn btn-outline-danger">Delete</a>

<h4>Details:</h4>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Attribute</th>
            <th scope="col">Value</th>
        </tr>
    </thead>
    <tbody>
        {{#each Role}}
        <tr>
            <th>{{@key}}</th>
            <td>{{{printKeyValue this}}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
