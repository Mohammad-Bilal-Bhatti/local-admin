
<h2>Secret - {{secretId}}</h2>

<a role="button" href="/secrets-manager/remove?secretId={{secretId}}" class="btn btn-outline-danger">Delete</a>

<h4 class="mt-2">Details:</h4>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Attribute</th>
            <th scope="col">Value</th>
        </tr>
    </thead>
    <tbody>
        {{#each details}}
        <tr>
            <th>{{@key}}</th>
            <td>{{{printKeyValue this}}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
