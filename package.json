{
  "name": "localstack-admin",
  "version": "1.0.0",
  "description": "Localstack admin build using nestjs MVC",
  "author": "Mohammad Bilal",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@angular-devkit/core": "^18.2.1",
    "@angular-devkit/schematics": "^18.2.1",
    "@aws-sdk/client-acm": "^3.637.0",
    "@aws-sdk/client-api-gateway": "^3.650.0",
    "@aws-sdk/client-dynamodb": "^3.631.0",
    "@aws-sdk/client-ec2": "^3.641.0",
    "@aws-sdk/client-eventbridge": "^3.633.0",
    "@aws-sdk/client-iam": "^3.637.0",
    "@aws-sdk/client-kms": "^3.629.0",
    "@aws-sdk/client-lambda": "^3.637.0",
    "@aws-sdk/client-s3": "^3.629.0",
    "@aws-sdk/client-secrets-manager": "^3.631.0",
    "@aws-sdk/client-sns": "^3.631.0",
    "@aws-sdk/client-sqs": "^3.637.0",
    "@aws-sdk/client-ssm": "^3.632.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/platform-express": "^10.4.1",
    "axios": "^1.7.5",
    "date-fns": "^3.6.0",
    "express-handlebars": "^5.3.2",
    "express-session": "^1.18.0",
    "express-status-monitor": "^1.3.4",
    "lodash": "^4.17.21",
    "npm": "^10.8.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^10.4.1",
    "@types/express": "^4.17.11",
    "@types/express-handlebars": "^5.3.0",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^26.0.22",
    "@types/multer": "^1.4.11",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^29.7.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
